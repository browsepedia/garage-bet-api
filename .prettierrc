{
  "plugins": ["prettier-plugin-organize-imports"],
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "semi": true,
  "useTabs": false,
  "proseWrap": "always",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "css",
  "bracketSameLine": true
}
